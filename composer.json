{
    "name": "phpdish/daily-song-plugin",
    "type": "phpdish-plugin",
    "description": "Daily song plugin for phpdish application",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "phpdish/phpdish": "*@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6|^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PHPDish\\DailySongPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPDish\\DailySongPlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
